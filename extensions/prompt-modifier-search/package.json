{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prompt-modifier-search",
  "title": "Prompt Modifier Search",
  "description": "Quickly browse modifier effects when creating Midjourney prompts",
  "icon": "icon.png",
  "author": "Chenhe",
  "categories": [
    "Documentation",
    "Design Tools",
    "Developer Tools"
  ],
  "keywords": [
    "prompt",
    "modifier",
    "midjourney",
    "dall-e",
    "AI"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "terms",
      "title": "Browse Terminologies",
      "subtitle": "Midjourney",
      "description": "Browse common photography terms.",
      "mode": "view"
    },
    {
      "name": "photographers",
      "title": "Browse Photographers",
      "subtitle": "Midjourney",
      "description": "Browse styles of master photographers",
      "mode": "view"
    },
    {
      "name": "artists",
      "title": "Browse Artists",
      "subtitle": "Midjourney",
      "description": "Browse styles of famous artists.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "itemSize",
      "title": "Item Size",
      "description": "Size of each item in display grid.",
      "required": false,
      "type": "dropdown",
      "defaultValue": "3",
      "data": [
        {
          "title": "Large",
          "value": "2"
        },
        {
          "title": "Medium",
          "value": "3"
        },
        {
          "title": "Small",
          "value": "4"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}